{
  "name": "@claude-code/testdatagen",
  "version": "1.0.0",
  "description": "Production-grade test data generation tool with constraint satisfaction",
  "main": "dist/index.js",
  "bin": {
    "testdatagen": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli/index.ts",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:coverage": "jest --coverage",
    "benchmark": "ts-node benchmarks/performance.ts",
    "benchmark:10k": "ts-node benchmarks/performance.ts --size 10000",
    "benchmark:100k": "ts-node benchmarks/performance.ts --size 100000",
    "benchmark:1m": "ts-node benchmarks/performance.ts --size 1000000",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "test-data",
    "test-data-generation",
    "faker",
    "sql",
    "database",
    "fixtures",
    "mock-data"
  ],
  "author": "Claude Code",
  "license": "MIT",
  "dependencies": {
    "@faker-js/faker": "^8.4.0",
    "@prisma/client": "^5.8.0",
    "better-sqlite3": "^9.2.2",
    "commander": "^11.1.0",
    "inquirer": "^9.2.12",
    "js-yaml": "^4.1.0",
    "mysql2": "^3.7.0",
    "node-sql-parser": "^4.17.0",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.11",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prisma": "^5.8.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
